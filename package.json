{
  "name": "solidity-by-example",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://solidity-by-example.org",
  "dependencies": {
    "highlight.js": "^9.15.10",
    "highlightjs-solidity": "^1.0.7",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "cp-contracts": "node scripts/cp-contracts.js",
    "gen-html-pages": "node scripts/gen-html-pages.js",
    "gen-html-page": "node scripts/gen-html-page.js",
    "gen-pages": "npm run cp-contracts && npm run gen-html-pages"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "gh-pages": "^2.1.1",
    "marked": "^0.7.0",
    "mustache": "^3.1.0",
    "react-snap": "^1.23.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
